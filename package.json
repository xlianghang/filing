  },
  "homepage": "https://github.com/xlianghang/filing#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/emscripten": "^1.39.5",
    "@types/node": "^16.10.2",
    "prettycli": "^1.4.3",
    "rollup": "^2.58.0",
    "tsc": "^2.0.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
